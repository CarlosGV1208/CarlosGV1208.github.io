{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://api.worldbank.org/v2/country/kor;ind;mex;arg;usa/indicator/SP.DYN.TFRT.IN?format=json&per_page=1000",
    "format": {"property": "[1]"}
  },
  "width": 500,
  "height": 350,
  "mark": "line",
  "title": {
    "text": "Declining fertility in the world",
    "subtitle": "Total Fertility Rate Source: World Bank"
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "title": "year",
      "axis": {"grid": false}
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "title": "Births per woman"
    },
    "color": {
      "field": "country.value",
      "type": "nominal",
      "scale": {"scheme": "tableau20"},
      "legend": {"title": "Country", "orient": "top-right"}
    }
  },
  "config": {}
}