{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Inflation",
    "anchor": "start",
    "subtitle": "All items, % | Source: ONS through ECO API",
    "fontSize": "18",
    "subtitleFontSize": "12",
    "color": "#6e2782",
    "subtitleColor": "#000000"
  },
  "height": "300",
  "width": "400",
  "autosize": {"type": "fit", "contains": "padding"},
  "background": "#ffffff",
  "view": {"fill": "#ffffff", "strokeOpacity": 0},
  "data": {
    "url": "https://api.economicsobservatory.com/gbr/infl?vega",
    "format": {"type": "json"}
  },
  "mark": {"type": "line", "color": "#902aac", "strokeWidth": "1"},
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    }
  },
  "config": {}
}